<!DOCTYPE html>
<head>
 
    <!-- Metadata -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{% block metaTitle %}{% endblock %}</title>
 
    <!-- CSS Files -->
    <link href="/bundles/maallsadmin/css/bootstrap.min.css" rel="stylesheet">
    <link href="/bundles/maallsadmin/font-awesome/css/font-awesome.css" rel="stylesheet">
    <link href="/bundles/maallsadmin/css/animate.css" rel="stylesheet">
    <link href="/bundles/maallsadmin/css/style.css" rel="stylesheet">
    {% block stylesheets %}
    {% endblock %}
 
</head>

<body>
    <div id="wrapper">
        <nav class="navbar-default navbar-static-side" role="navigation">
            
            
            
            {% block sideNavContainer %}

                {% block sideNav %}
                {% endblock %}

            {% endblock %}
            
        </nav>

        <div id="page-wrapper" class="gray-bg dashbard-1">
            <div class="row border-bottom">
                <nav class="navbar navbar-static-top" role="navigation" style="margin-bottom: 0">
                    
                    <div class="navbar-header">
                        <a class="navbar-minimalize minimalize-styl-2 btn btn-primary " href="javascript:"><i class="fa fa-bars"></i> </a>
                        
                    </div>
                    <ul class="nav navbar-top-links navbar-right">
                        {% block topRightNav %}{% endblock %}
                        <li>
                            <a href="{{ path('fos_user_security_logout') }}">
                                <i class="fa fa-sign-out"></i> Log out
                            </a>
                        </li>
                    </ul>
                </nav>
                
                {% for type in ['success', 'notice', 'error'] %}
                    {% for message in app.session.flashbag.get(type) %}
                        <div class="alert alert-{{type}} alert-dismissable text-center">
                            <button aria-hidden="true" data-dismiss="alert" class="close" type="button">Ã—</button>
                            {{ message }}
                        </div>
                    {% endfor %}
                {% endfor %}
            </div>

            {% block mainContainer %}

                {% block main %}
                {% endblock %}
                
            {% endblock %}
        </div>
    </div>

    <!-- Mainly scripts -->
    <script src="/bundles/maallsadmin/js/jquery-2.1.1.js"></script>
    <script src="/bundles/maallsadmin/js/bootstrap.min.js"></script>
    <script src="/bundles/maallsadmin/js/plugins/metisMenu/jquery.metisMenu.js"></script>
    <script src="/bundles/maallsadmin/js/plugins/slimscroll/jquery.slimscroll.min.js"></script>

    <!-- Flot -->
    <script src="/bundles/maallsadmin/js/plugins/flot/jquery.flot.js"></script>
    <script src="/bundles/maallsadmin/js/plugins/flot/jquery.flot.tooltip.min.js"></script>
    <script src="/bundles/maallsadmin/js/plugins/flot/jquery.flot.spline.js"></script>
    <script src="/bundles/maallsadmin/js/plugins/flot/jquery.flot.resize.js"></script>
    <script src="/bundles/maallsadmin/js/plugins/flot/jquery.flot.pie.js"></script>

    <!-- Peity -->
    <script src="/bundles/maallsadmin/js/plugins/peity/jquery.peity.min.js"></script>
    <script src="/bundles/maallsadmin/js/demo/peity-demo.js"></script>

    <!-- Custom and plugin javascript -->
    <script src="/bundles/maallsadmin/js/inspinia.js"></script>
    <script src="/bundles/maallsadmin/js/plugins/pace/pace.min.js"></script>

    <!-- jQuery UI -->
    <script src="/bundles/maallsadmin/js/plugins/jquery-ui/jquery-ui.min.js"></script>

    <!-- GITTER -->
    <script src="/bundles/maallsadmin/js/plugins/gritter/jquery.gritter.min.js"></script>

    <!-- EayPIE -->
    <script src="/bundles/maallsadmin/js/plugins/easypiechart/jquery.easypiechart.js"></script>

    <!-- Sparkline -->
    <script src="/bundles/maallsadmin/js/plugins/sparkline/jquery.sparkline.min.js"></script>

    <!-- Sparkline demo data  -->
    <script src="/bundles/maallsadmin/js/demo/sparkline-demo.js"></script>

    <!-- ChartJS-->
    <script src="/bundles/maallsadmin/js/plugins/chartJs/Chart.min.js"></script>
    {% block javascripts %}
    {% endblock %}
</body>
</html>
